<?xml version="1.0" encoding="UTF-8"?>

<main>
<sensors>
    <sensor>
        <sensorName>Distance Sensor</sensorName>
        <sensorMake>Honeywell</sensorMake>
        <streamRate>1</streamRate>
        <sensorDataType>Vector</sensorDataType>
        <format>60*1</format>
        <sensorSupport>twoway</sensorSupport>
        <sensorType>DISTANCE</sensorType>
    </sensor>
    <sensor>
        <sensorName>Sonar Sensor</sensorName>
        <sensorMake>Honeywell</sensorMake>
        <streamRate>1</streamRate>
        <sensorDataType>Scalar</sensorDataType>
        <format>None</format>
        <sensorSupport>oneway</sensorSupport>
        <sensorType>SONAR</sensorType>
    </sensor>
    <sensor>
        <sensorName>Sonar Sensor</sensorName>
        <sensorMake>Honeywell</sensorMake>
        <streamRate>1</streamRate>
        <sensorDataType>Scalar</sensorDataType>
        <format>None</format>
        <sensorSupport>oneway</sensorSupport>
        <sensorType>IRIS</sensorType>
    </sensor>
    <sensor>
        <sensorName>Camera Sensor</sensorName>
        <sensorMake>Honeywell</sensorMake>
        <streamRate>1</streamRate>
        <sensorDataType>Scalar</sensorDataType>
        <format>None</format>
        <sensorSupport>oneway</sensorSupport>
        <sensorType>CAMERA</sensorType>
    </sensor>
</sensors>

<services>
    <service>
        <serviceName>dbService</serviceName>
        <startUPFile>dbservice.py</startUPFile>
        <type>shared</type>
        <minInstances>1</minInstances>
        <minResponseTime>100ms</minResponseTime>
        <highMark>50</highMark>
        <lowMark>10</lowMark>
        <maxInstances>1</maxInstances>
    </service>
    <service>
        <serviceName>ServerStats</serviceName>
        <startUPFile>ServerStats.py</startUPFile>
        <type>shared</type>
        <minInstances>1</minInstances>
        <minResponseTime>100ms</minResponseTime>
        <highMark>50</highMark>
        <lowMark>10</lowMark>
        <maxInstances>1</maxInstances>
    </service>
    <service>
        <serviceName>LoadBalancer</serviceName>
        <startUPFile>LoadBalancer.py</startUPFile>
        <type>shared</type>
        <minInstances>1</minInstances>
        <minResponseTime>100ms</minResponseTime>
        <highMark>50</highMark>
        <lowMark>10</lowMark>
        <maxInstances>1</maxInstances>
    </service>
    <service>
        <serviceName>dbHelperService</serviceName>
        <startUPFile>dbHelperService.py</startUPFile>
        <type>shared</type>
        <minInstances>1</minInstances>
        <minResponseTime>100ms</minResponseTime>
        <highMark>50</highMark>
        <lowMark>10</lowMark>
        <maxInstances>1</maxInstances>
        <dependencies>
            <service-dependency>
                <service-name>dbService</service-name>
            </service-dependency>
        </dependencies>
    </service>
    <service>
        <serviceName>deployService</serviceName>
        <startUPFile>deploy_service.py</startUPFile>
        <type>shared</type>
        <minInstances>1</minInstances>
        <minResponseTime>100ms</minResponseTime>
        <highMark>50</highMark>
        <lowMark>10</lowMark>
        <maxInstances>1</maxInstances>
        <dependencies>
            <service-dependency>
                <service-name>dbService</service-name>
                <service-name>LoadBalancer</service-name>
            </service-dependency>
        </dependencies>
    </service>
    <service>
        <serviceName>ScheduleService</serviceName>
        <startUPFile>schedule_service.py</startUPFile>
        <type>shared</type>
        <minInstances>1</minInstances>
        <minResponseTime>100ms</minResponseTime>
        <highMark>50</highMark>
        <lowMark>10</lowMark>
        <maxInstances>1</maxInstances>
        <dependencies>
            <service-dependency>
                <service-name>dbService</service-name>
                <service-name>LoadBalancer</service-name>
            </service-dependency>
        </dependencies>
    </service>
    <service>
        <serviceName>requestManager</serviceName>
        <startUPFile>request_manager.py</startUPFile>
        <type>shared</type>
        <minInstances>1</minInstances>
        <minResponseTime>100ms</minResponseTime>
        <highMark>50</highMark>
        <lowMark>10</lowMark>
        <maxInstances>1</maxInstances>
        <dependencies>
            <service-dependency>
                <service-name>dbService</service-name>
                <service-name>LoadBalancer</service-name>
            </service-dependency>
        </dependencies>
    </service>
    <service>
            <serviceName>MachineAgent</serviceName>
            <startUPFile>MachineAgent.py</startUPFile>
            <type>shared</type>
            <minInstances>1</minInstances>
            <minResponseTime>100ms</minResponseTime>
            <highMark>50</highMark>
            <lowMark>10</lowMark>
            <maxInstances>1</maxInstances>
            <dependencies>
                <service-dependency>
                    <service-name>LoadBalancer</service-name>
                </service-dependency>
            </dependencies>
    </service>
    <service>
        <serviceName>HealthCheck</serviceName>
        <startUPFile>HealthCheck.py</startUPFile>
        <type>shared</type>
        <minInstances>1</minInstances>
        <minResponseTime>100ms</minResponseTime>
        <highMark>50</highMark>
        <lowMark>10</lowMark>
        <maxInstances>1</maxInstances>
        <dependencies>
            <service-dependency>
                <service-name>LoadBalancer</service-name>
                <service-name>dbHelperService</service-name>
                <service-name>scheduleService</service-name>
            </service-dependency>
        </dependencies>
    </service>
    <service>
        <serviceName>MonitoringService</serviceName>
        <startUPFile>MonitoringService.py</startUPFile>
        <type>shared</type>
        <minInstances>1</minInstances>
        <minResponseTime>100ms</minResponseTime>
        <highMark>50</highMark>
        <lowMark>10</lowMark>
        <maxInstances>1</maxInstances>
        <dependencies>
            <service-dependency>
                <service-name>LoadBalancer</service-name>
                <service-name>ServerStats</service-name>
            </service-dependency>
        </dependencies>
    </service>
    <service>
        <serviceName>Logging</serviceName>
        <startUPFile>Logging.py</startUPFile>
        <type>shared</type>
        <minInstances>1</minInstances>
        <minResponseTime>100ms</minResponseTime>
        <highMark>50</highMark>
        <lowMark>10</lowMark>
        <maxInstances>1</maxInstances>
    </service>
</services>
</main>