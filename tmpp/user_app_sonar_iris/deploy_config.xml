<models>
    <model>
        <modelName>iris</modelName>
        <predURLStructure>v1/models/iris/versions/1</predURLStructure>
        <fileName>iris.pb</fileName>
        <type>shared</type>
        <dependencies>
            <sensor-type>IRIS</sensor-type>
            <sensor-type>SONAR</sensor-type>
            <sensor-type>DISTANCE</sensor-type>
        </dependencies>
    </model>
    <model>
        <modelName>Sonar</modelName>
        <predURLStructure>v1/models/Sonar/versions/1</predURLStructure>
        <fileName>Sonar.pb</fileName>
        <type>exclusive</type>
        <dependencies>
            <sensor-type>SONAR</sensor-type>
            <sensor-type>IRIS</sensor-type>
            <sensor-type>DISTANCE</sensor-type>
        </dependencies>
    </model>
</models>

<services>
    <service>
        <serviceName>twowaySensorService</serviceName>
        <startUPFile>twowaySensorService.py</startUPFile>
        <type>shared</type>
        <dependencies>
            <sensor-dependency>
                <sensor-type>DISTANCE</sensor-type>
            </sensor-dependency>
        </dependencies>
    </service>
	<service>
        <serviceName>notificationService</serviceName>
        <startUPFile>notificationService.py</startUPFile>
        <type>shared</type>
    </service>
    <service>
        <serviceName>distanceAlarmService</serviceName>
        <startUPFile>distanceAlarmService.py</startUPFile>
        <type>shared</type>
        <dependencies>
            <service-dependency>
                <service-name>notificationService</service-name>
            </service-dependency>
            <sensor-dependency>
            	<sensor-type>DISTANCE</sensor-type>
            </sensor-dependency>
        </dependencies>
    </service>
     <service>
        <serviceName>counterService</serviceName>
        <startUPFile>counterService.py</startUPFile>
        <type>shared</type>
        <dependencies>
            <model-dependency>
                <model-name>Sonar</model-name>
            </model-dependency>
            <sensor-dependency>
            	<sensor-type>SONAR</sensor-type>
            </sensor-dependency>
        </dependencies>
    </service>
     <service>
        <serviceName>helperService</serviceName>
        <startUPFile>helperService.py</startUPFile>
        <type>exclusive</type>
        <dependencies>
        	<sensor-dependency>
            	<sensor-type>IRIS</sensor-type>
            </sensor-dependency>
            <model-dependency>
                <model-name>iris</model-name>
            </model-dependency>
        </dependencies>
    </service>
</services>
</main>